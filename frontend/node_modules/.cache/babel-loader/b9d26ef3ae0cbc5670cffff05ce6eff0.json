{"ast":null,"code":"import axios from 'axios';\nexport default {\n  name: 'App',\n\n  data() {\n    return {\n      login: false,\n      username: '',\n      password: '',\n      user_data: ''\n    };\n  },\n\n  beforeCreate() {\n    this.$store.commit('initializeStore');\n    const access = this.$store.state.access;\n\n    if (access) {\n      axios.defaults.headers.common['Authorization'] = 'JWT ' + access;\n      this.login = true;\n      console.log('login');\n    } else {\n      axios.defaults.headers.common['Authorization'] = '';\n      this.login = false;\n      console.log('no login');\n    }\n  },\n\n  mounted() {\n    setInterval(() => {\n      this.getAcces();\n    }, 59000);\n  },\n\n  methods: {\n    getAcces(e) {\n      const accessData = {\n        refresh: this.$store.state.refresh\n      };\n      axios.post('/api/v1/jwt/refresh/', accessData).then(response => {\n        const access = response.data.access;\n        this.$store.commit('setAccess', access);\n        axios.defaults.headers.common['Authorization'] = 'JWT ' + access;\n        localStorage.setItem('access', access);\n      }).catch(error => {\n        console.log(error);\n      });\n    },\n\n    doLogin(e) {\n      const loginData = {\n        username: this.username,\n        password: this.password\n      };\n      axios.post('/api/v1/jwt/create/', loginData).then(response => {\n        const access = response.data.access;\n        this.$store.commit('setAccess', access);\n        axios.defaults.headers.common['Authorization'] = 'JWT ' + access;\n        localStorage.setItem('access', access);\n        this.login = true;\n        this.username = '', this.password = '';\n      }).catch(error => {\n        console.log(error);\n      });\n      axios.get('/api/v1/users/me/').then(response => {\n        this.user_data = response.data.username;\n      }).catch(error => {\n        console.log(error);\n      });\n    },\n\n    doLogout(e) {\n      this.$store.commit('setAccess', '');\n      axios.defaults.headers.common['Authorization'] = '';\n      localStorage.removeItem('access');\n      this.user_data = '';\n      this.login = false;\n    }\n\n  }\n};","map":{"version":3,"mappings":"AA4DA,OAAOA,KAAP,MAAkB,OAAlB;AACA,eAAc;EACZC,IAAI,EAAE,KADM;;EAEZC,IAAI,GAAG;IACL,OAAO;MACLC,KAAK,EAAE,KADF;MAELC,QAAQ,EAAE,EAFL;MAGLC,QAAQ,EAAE,EAHL;MAILC,SAAS,EAAC;IAJL,CAAP;EAMD,CATW;;EAUZC,YAAY,GAAE;IACZ,KAAKC,MAAL,CAAYC,MAAZ,CAAmB,iBAAnB;IACA,MAAMC,MAAK,GAAI,KAAKF,MAAL,CAAYG,KAAZ,CAAkBD,MAAjC;;IACA,IAAGA,MAAH,EAAU;MACRV,KAAK,CAACY,QAAN,CAAeC,OAAf,CAAuBC,MAAvB,CAA8B,eAA9B,IAAiD,SAASJ,MAA1D;MACA,KAAKP,KAAL,GAAa,IAAb;MACAY,OAAO,CAACC,GAAR,CAAY,OAAZ;IACD,CAJD,MAIK;MACHhB,KAAK,CAACY,QAAN,CAAeC,OAAf,CAAuBC,MAAvB,CAA8B,eAA9B,IAAiD,EAAjD;MACA,KAAKX,KAAL,GAAa,KAAb;MACAY,OAAO,CAACC,GAAR,CAAY,UAAZ;IACF;EACD,CAtBW;;EAuBZC,OAAO,GAAE;IACPC,WAAW,CAAC,MAAI;MACd,KAAKC,QAAL;IACD,CAFU,EAET,KAFS,CAAX;EAGD,CA3BW;;EA4BZC,OAAO,EAAC;IACND,QAAQ,CAACE,CAAD,EAAG;MACT,MAAMC,UAAU,GAAC;QACfC,OAAO,EAAE,KAAKf,MAAL,CAAYG,KAAZ,CAAkBY;MADZ,CAAjB;MAGAvB,KAAK,CAACwB,IAAN,CAAW,sBAAX,EAAkCF,UAAlC,EACKG,IADL,CACUC,QAAQ,IAAE;QACd,MAAMhB,MAAK,GAAIgB,QAAQ,CAACxB,IAAT,CAAcQ,MAA7B;QACA,KAAKF,MAAL,CAAYC,MAAZ,CAAmB,WAAnB,EAA+BC,MAA/B;QACAV,KAAK,CAACY,QAAN,CAAeC,OAAf,CAAuBC,MAAvB,CAA8B,eAA9B,IAA+C,SAAOJ,MAAtD;QACAiB,YAAY,CAACC,OAAb,CAAqB,QAArB,EAA8BlB,MAA9B;MACD,CANL,EAOKmB,KAPL,CAOWC,KAAK,IAAE;QACZf,OAAO,CAACC,GAAR,CAAYc,KAAZ;MACD,CATL;IAUD,CAfK;;IAgBNC,OAAO,CAACV,CAAD,EAAG;MACR,MAAMW,SAAS,GAAC;QACd5B,QAAQ,EAAE,KAAKA,QADD;QAEdC,QAAQ,EAAE,KAAKA;MAFD,CAAhB;MAIAL,KAAK,CAACwB,IAAN,CAAW,qBAAX,EAAiCQ,SAAjC,EACKP,IADL,CACUC,QAAQ,IAAE;QACd,MAAMhB,MAAK,GAAIgB,QAAQ,CAACxB,IAAT,CAAcQ,MAA7B;QACA,KAAKF,MAAL,CAAYC,MAAZ,CAAmB,WAAnB,EAA+BC,MAA/B;QACAV,KAAK,CAACY,QAAN,CAAeC,OAAf,CAAuBC,MAAvB,CAA8B,eAA9B,IAA+C,SAAOJ,MAAtD;QACAiB,YAAY,CAACC,OAAb,CAAqB,QAArB,EAA8BlB,MAA9B;QACA,KAAKP,KAAL,GAAa,IAAb;QACA,KAAKC,QAAL,GAAc,EAAd,EACA,KAAKC,QAAL,GAAc,EADd;MAED,CATL,EAUKwB,KAVL,CAUWC,KAAK,IAAE;QACZf,OAAO,CAACC,GAAR,CAAYc,KAAZ;MACD,CAZL;MA